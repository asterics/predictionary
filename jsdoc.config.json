{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.mjs(doc|x)?$"
  },
  "opts": {
    "destination": "docs"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}